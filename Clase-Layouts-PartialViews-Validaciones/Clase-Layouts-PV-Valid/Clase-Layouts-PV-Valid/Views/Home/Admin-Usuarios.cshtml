@{
    Layout = "_AdminLayout";
}
@section TopContent
{
    <h2>Admin Usuarios</h2>
}
<div>
    Usuario con más puntos:
    <partial name="_pvLinkUsuario" model="@("usuario 5")"/>
</div>
<div>
    <table>
        <tr>
            <th>Usuario</th>
            <th>Email</th>
        </tr>
        @for (int i = 0; i < 10; i++)
        {
    <tr>
        <td>
            <partial name="_pvLinkUsuario" model="@($"usuario {i}")" />

        </td>
        <td>email@(i)@@gmail.com</td>
    </tr>
}
    </table>
</div>

@section Footer
{
    <span>Hecho en casa el día 02/05/2022</span>
}